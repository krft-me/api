<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Order.
    -->
    <changeSet id="20231127190258-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="review_id"
                                 baseTableName="krftme_order"
                                 constraintName="fk_krftme_order__review_id"
                                 referencedColumnNames="id"
                                 referencedTableName="review"/>

        <addForeignKeyConstraint baseColumnNames="offer_id"
                                 baseTableName="krftme_order"
                                 constraintName="fk_krftme_order__offer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_user_offer"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="krftme_order"
                                 constraintName="fk_krftme_order__customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="application_user"/>
    </changeSet>
</databaseChangeLog>
